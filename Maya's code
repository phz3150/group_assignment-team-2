import numpy as np
def volume(R):
    """Calculates the volume of a sphere given a radius R
    Input: Radius
    Output: Volume of Sphere"""
    
    v = (4/3) * np.pi * (R**3)
    
    return v

x = np.arange(-10,10,0.5)
